#include"stdafx.h" 
#include <iostream> 
#include <string> 
using namespace std;

struct car {
	std::string model, num, name;
};

car CreateCar() {
	car NewCar;
	std::cin >> NewCar.model >> NewCar.num >> NewCar.name;
	return NewCar;
}

void SelectCar(car List[], int n, std::string key) {
	for (int i = 0; i<n; i++)
		if (List[i].model == key)
			std::cout << List[i].num << ' ' << List[i].name << std::endl;

}

int main()
{
	const int n = 2;
	car List[n];
	std::string key;
	for (int i = 0; i < n; i++)
		List[i] = CreateCar();
	std::cout << "enter model: ";
	std::cin >> key;
	SelectCar(List, n, key);
	std::cin >> key;
	return 0;
}
